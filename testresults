                },
                "should_succeed": False,  # Should be rejected
            },
            {
                "name": "Unicode and special characters",
                "data": {
                    "username": "unicode_",
                    "email": "unicode@example.com",
                    "password": "ValidPass123!",
                    "full_name": "Unicode  "
                },
                "should_succeed": False,  # Username should be rejected
